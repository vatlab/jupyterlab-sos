{
  "title": "jupyterlab-sos",
  "description": "SoS extension settings.",
  "jupyter.lab.toolbars": {
    "Cell": [
      {
        "name": "kernel_selector"
      }
    ]
  },
  "jupyter.lab.shortcuts": [
    {
      "command": "sos:toggle_output",
      "keys": ["Ctrl Shift O"],
      "selector": ".jp-Notebook.jp-mod-editMode"
    },
    {
      "command": "sos:toggle_kernel",
      "keys": ["Ctrl Shift S"],
      "selector": ".jp-Notebook.jp-mod-editMode"
    },
    {
      "command": "sos:toggle_markdown",
      "keys": ["Ctrl Shift M"],
      "selector": ".jp-Notebook.jp-mod-editMode"
    },
    {
      "command": "notebook:run-in-console",
      "keys": ["Ctrl Shift Enter"],
      "selector": ".jp-Notebook.jp-mod-editMode"
    }
  ],
  "properties": {
    "sos.kernel_codemirror_mode": {
      "title": "CodeMirror mode for SoS kernels",
      "description": "SoS action - codemirror mode map. The mode can be a single string or an object.",
      "default": {
        "python": {
          "name": "python",
          "version": 3
        },
        "python2": {
          "name": "python",
          "version": 2
        },
        "python3": {
          "name": "python",
          "version": 3
        },
        "r": "r",
        "report": "report",
        "pandoc": "markdown",
        "download": "markdown",
        "markdown": "markdown",
        "ruby": "ruby",
        "sas": "sas",
        "bash": "shell",
        "sh": "shell",
        "julia": "julia",
        "run": "shell",
        "javascript": "javascript",
        "typescript": {
          "name": "javascript",
          "typescript": true
        },
        "octave": "octave",
        "matlab": "octave",
        "mllike": "mllike",
        "clike": "clike",
        "html": "htmlembedded",
        "xml": "xml",
        "yaml": "yaml",
        "json": {
          "name": "javascript",
          "jsonMode": true
        },
        "stex": "stex"
      }
    }
  },
  "additionalProperties": false,
  "type": "object"
}
